### Function
add3c <- function(
name,
position,
label_first_only = F
) {
if(label_first_only == F) {
ph_with_chart_at(
doc,
chart = name,
left = case_when(
position == 'topright' ~ 4.375,
position == 'righttop' ~ 4.375,
position == 'bottomright' ~ 4.375,
position == 'rightbottom' ~ 4.375,
position == 'left' ~ .0,
position == 'center' ~ 4.25,
position == 'middle' ~ 4.25,
position == 'right' ~ 8.5
),
top = case_when(
position == 'topright' ~ 2,
position == 'righttop' ~ 2,
position == 'bottomright' ~ 4.5,
position == 'rightbottom' ~ 4.5,
position == 'left' ~ 2,
position == 'center' ~ 2,
position == 'middle' ~ 2,
position == 'right' ~ 2
),
height = case_when(
position == 'topright' ~ 3,
position == 'righttop' ~ 3,
position == 'bottomright' ~ 3,
position == 'rightbottom' ~ 3,
position == 'left' ~ 5.5,
position == 'center' ~ 5.5,
position == 'middle' ~ 5.5,
position == 'right' ~ 5.5
),
width = case_when(
position == 'topright' ~ 8.615,
position == 'righttop' ~ 8.615,
position == 'bottomright' ~ 8.615,
position == 'rightbottom' ~ 8.615,
position == 'left' ~ 4.5,
position == 'center' ~ 4.5,
position == 'middle' ~ 4.5,
position == 'right' ~ 4.5
)
)
} else{ #label_first_only == T
ph_with_chart_at(
doc,
chart = name,
left = case_when(
position == 'topright' ~ 4.375,
position == 'righttop' ~ 4.375,
position == 'bottomright' ~ 4.375,
position == 'rightbottom' ~ 4.375,
position == 'left' ~ .0,
position == 'center' ~ 5.875,
position == 'middle' ~ 5.875,
position == 'right' ~ 9.5
),
top = case_when(
position == 'topright' ~ 2,
position == 'righttop' ~ 2,
position == 'bottomright' ~ 4.5,
position == 'rightbottom' ~ 4.5,
position == 'left' ~ 2,
position == 'center' ~ 2,
position == 'middle' ~ 2,
position == 'right' ~ 2
),
height = case_when(
position == 'topright' ~ 3,
position == 'righttop' ~ 3,
position == 'bottomright' ~ 3,
position == 'rightbottom' ~ 3,
position == 'left' ~ 5.5,
position == 'center' ~ 5.5,
position == 'middle' ~ 5.5,
position == 'right' ~ 5.5
),
width = case_when(
position == 'topright' ~ 8.615,
position == 'righttop' ~ 8.615,
position == 'bottomright' ~ 8.615,
position == 'rightbottom' ~ 8.615,
position == 'left' ~ 6,
position == 'center' ~ 3.75,
position == 'middle' ~ 3.75,
position == 'right' ~ 3.75
)
)
}
}
